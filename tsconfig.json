{
  "compilerOptions": {
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "moduleResolution": "node",
    "stripInternal": false,
    "target": "es5",
    "outDir": "./.out",
    "lib": [
      "es7"
    ]
  },
  "formatCodeOptions": {
    "indentSize": 2,
    "tabSize": 2
  }
}
